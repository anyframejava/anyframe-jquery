<?xml version="1.0" encoding="UTF-8"?>
<queryservice xmlns="http://www.anyframejava.org/schema/query/mapping" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.anyframejava.org/schema/query/mapping http://www.anyframejava.org/schema/query/mapping/anyframe-query-mapping-1.0.xsd ">

  <queries>
	<query id="createJqueryAttached">
		<statement>
		<![CDATA[
			INSERT INTO JQUERY_ATTACHED ( 			
				ID, 
				NAME, 
				FILE_SIZE, 
				REF_ID) 
			VALUES (:vo.id, 
				:vo.name, 
				:vo.fileSize, 
				:vo.refId)
		]]>
		</statement>
	</query>
		
	<query id="updateRefIdJqueryAttached">
		<statement>
		<![CDATA[
			UPDATE JQUERY_ATTACHED
			SET 
				REF_ID = :newRefId
			WHERE
				REF_ID = :refId 
		]]>
		</statement>
	</query>
	
	<query id="removeJqueryAttached">
		<statement>
		<![CDATA[
			DELETE FROM JQUERY_ATTACHED 
			WHERE 
				ID = :vo.id
		]]>
		</statement>
	</query>
	
	<query id="findJqueryAttachedByPk">
		<statement>
		<![CDATA[
			SELECT 
				ID, NAME, FILE_SIZE, REF_ID
			FROM JQUERY_ATTACHED jqueryFileUpload
			WHERE 
				ID = :vo.id
		]]>
		</statement>
		<result class="org.anyframe.plugin.jquery.domain.Attached"/>		
	</query>

	<query id="selectJqueryAttachedListByRefId" isDynamic="true">
		<statement>
		<![CDATA[
			SELECT 
				ID, NAME, FILE_SIZE, REF_ID 
			FROM JQUERY_ATTACHED
				WHERE 		
					REF_ID = :refId			
				order by							
				ID
		]]>
		</statement>
		<result class="org.anyframe.plugin.jquery.domain.Attached"/>		
	</query>
  </queries>
</queryservice>
