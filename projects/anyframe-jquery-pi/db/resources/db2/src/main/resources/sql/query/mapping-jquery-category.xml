				<!--jquery-findJqueryCategoryList-START-->
				SELECT 
					CATEGORY_ID,
					CATEGORY_NAME,
					CATEGORY_DESC,
					REG_DATE
				FROM JQUERY_CATEGORY
				WHERE 1 = 1
				<!--Velocity-Support-dynamicFindJqueryCategoryList-START-->
				#if($vo.categoryName &amp;&amp; !$vo.categoryName.equals(""))
					AND CATEGORY_NAME like '%' CONCAT :vo.categoryName CONCAT '%'
				#end
				#if($vo.categoryDesc &amp;&amp; !$vo.categoryDesc.equals(""))
					AND CATEGORY_DESC like '%' CONCAT :vo.categoryDesc CONCAT '%'
				#end
				#if($vo.regDate &amp;&amp; !$vo.regDate.equals(""))
					AND REG_DATE like :vo.regDate CONCAT '%'
				#end
				ORDER BY
				#if($vo.sidx &amp;&amp; !$vo.sidx.equals(""))
					#if($vo.sidx == "categoryId")
						CATEGORY_ID
					#elseif($vo.sidx == "categoryName")
						CATEGORY_NAME	
					#elseif($vo.sidx == "categoryDesc")
						CATEGORY_DESC
					#elseif($vo.sidx == "regDate")
						REG_DATE
					#end
					#if($vo.sord == "desc" || $vo.sord == "")
						DESC
					#else
						ASC
					#end
				#else
					CATEGORY_ID
				#end
				<!--Velocity-Support-dynamicFindJqueryCategoryList-END-->
				<!--jquery-findJqueryCategoryList-END-->